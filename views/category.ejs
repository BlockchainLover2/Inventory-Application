<head>
    <link rel="stylesheet" href="../styles.css">
    <title>jjj</title>
</head>



<div class="item-container">
    <%items.forEach(x=>{ %>
        <%- include("partials/item", {item:x,category:category})%>
    <% })%>


        <form action="/category/<%=category%>/create" method="get">
            <button type="submit">Create Item</button>
        </form>



    <form action="/category/<%=category%>/delete" method="post">
        <button type="submit"
                onclick="return confirm('Are you sure you want to delete this category?');">Delete Category</button>
    </form>
</div>